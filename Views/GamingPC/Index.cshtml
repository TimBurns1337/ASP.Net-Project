@model List<GamingPC>@{ string title = ViewBag.Action;                ViewBag.Title = title;}<h2>Index</h2><a asp-controller="GamingPC" asp-action="Create">Create New</a><table class="table table-bordered table-striped">    <thead>        <tr>            <th>Pc Id</th>            <th>Summary</th>            <th>Image Link</th>            <th>Price</th>            <th>Options</th>        </tr>    </thead>    @foreach (var pcList in Model)    {<tbody>    <tr>        <td>@pcList.PcId</td>        <td>@pcList.Summary</td>        <td><img src="~/images/@pcList.ImageLink" alt="PC 1" style="width:42px;height:42px;"></td>        <td>@pcList.Price</td>        <td>            <a asp-controller="GamingPC" asp-action="Edit" asp-route-id="@pcList.PcId">Edit</a> |            <a asp-controller="GamingPC" asp-action="Details" asp-route-id="@pcList.PcId">Details</a> |            <a asp-controller="GamingPC" asp-action="Delete" asp-route-id="@pcList.PcId">Delete</a>        </td>    </tr></tbody>    }</table>